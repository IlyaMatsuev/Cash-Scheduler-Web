<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Balance_Cannot_Be_Less_Than_Zero</fullName>
    <active>true</active>
    <errorConditionFormula>Balance__c &lt; 0</errorConditionFormula>
    <errorDisplayField>Balance__c</errorDisplayField>
    <errorMessage>Wallet balance cannot be less than zero</errorMessage>
</ValidationRule>
