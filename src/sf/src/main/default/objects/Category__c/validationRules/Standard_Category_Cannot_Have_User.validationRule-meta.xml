<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Standard_Category_Cannot_Have_User</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(IsCustom__c) &amp;&amp; NOT(ISBLANK(User__c))</errorConditionFormula>
    <errorDisplayField>User__c</errorDisplayField>
    <errorMessage>The standard category cannot have a user assigned</errorMessage>
</ValidationRule>
